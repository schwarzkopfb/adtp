{
  "name": "conglo",
  "version": "0.3.5",
  "description": "compose expressive async data transformation pipelines",
  "main": "lib/with-globals.js",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "test": "test",
    "example": "examples"
  },
  "files": [
    "lib/**",
    "script/**"
  ],
  "scripts": {
    "preversion": "npm test",
    "postversion": "npm run changelog",
    "postpublish": "git push origin --all; git push origin --tags",
    "postinstall": "node script/postinstall",
    "changelog": "standard-changelog",
    "test": "tap test/**",
    "test:cov": "npm test -- --coverage-report=lcov",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "cloc": "cloc lib lib/global lib/without-globals.js",
    "cloc:test": "cloc test",
    "cloc:docs": "cloc docs examples",
    "repl": "node -r ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schwarzkopfb/conglo.git"
  },
  "keywords": [
    "array",
    "async",
    "chain",
    "collection",
    "data",
    "enumerable",
    "generator",
    "iterable",
    "iterator",
    "manipulation",
    "pipeline",
    "promise",
    "set",
    "transform"
  ],
  "author": "Schwarzkopf Bal√°zs <schwarzkopfb@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schwarzkopfb/conglo/issues"
  },
  "homepage": "https://github.com/schwarzkopfb/conglo#readme",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-function-bind": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "cloc": "^2.5.1",
    "husky": "^3.1.0",
    "is-ci": "^2.0.0",
    "standard-changelog": "^2.0.18",
    "tap": "^14.10.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
